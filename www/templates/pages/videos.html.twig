{% extends 'base.html.twig' %}

{% block body %}
    <div data-page="videos" class="page no-toolbar no-navbar">
        <div class="page-content">

            <div class="navbarpages navbarpagesbg">
                <div class="navbar_left">
                    <a href="{{ path('app_home') }}"><img src="{{ asset('images/icons/white/home.png') }}" alt=""
                                                          style="width:25px" title=""/></a>
                </div>
                <div class="navbar_right navbar_right_menu">
                    <a href="#" data-panel="left" class="open-panel"><img
                                src="{{ asset('images/icons/white/menu.png') }}" alt="" title=""/></a>
                </div>

            </div>


            <div id="pages_maincontent">

                <h2 class="page_title">MWPF VIDEOS</h2>

                <div class="page_single layout_fullwidth_padding">
                    <div class="swiper-wrapper">

                            <div class="swiper-slide">
                                <img src="{{ asset('images/mwpfslider5.jpg') }}" alt="" title=""/>
                            </div>
                         
                    </div>
                    <div class="videocontainer">
                        <iframe width="100%" height="180" src="https://websitedev.mwpf.co.za:92/video/mwpf-english.mp4"
                                frameborder="0"></iframe>
                    </div>
                    <h3 class="page_subtitle">The Fund of Choice - English</h3>

                    <div class="videocontainer">
                        <iframe width="100%" height="180" src="https://websitedev.mwpf.co.za:92/video/mwpf-zulu.mp4"
                                frameborder="0"></iframe>
                    </div>
                    <h3 class="page_subtitle">The Fund of Choice - IsiZulu</h3>

                    <div class="videocontainer">
                        <iframe width="100%" height="180" src="https://websitedev.mwpf.co.za:92/video/mwpf-sesotho.mp4"
                                frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
                    </div>
                    <h3 class="page_subtitle">The Fund of Choice - SeSotho</h3>
                </div>

            </div>


        </div>
    </div>
{% endblock %}
